{{debug}}

<a id="Crime &amp; Public Safety">
    <h4 class="section-title">Crime &amp; Public Safety</h4>
</a>
    {{#objects}}
        {{#if projectorder}}
            {{#is projecttopic 'Crime & Public Safety'}}
                <div class="row-fluid">
                    <div class="{{projecttopic}}">
                        {{#if projectimage}}
                            <div class="span3">
                                <h4 class="kicker">{{projecttype}}</h4>
                                <img src="{{projectimage}}" alt="{{projecttitle}}" />
                            </div>
                            <div class="span9">
                                <h3><a href="{{promotedurl}}" target="_blank">{{projecttitle}}</a></h3>
                                <p><span class="data-instructions">By {{projectbylines}}</span></p>
                                <p>{{truncate projectdescription 250 " ... "}}<br />
                                {{#if readmoreurl}}
                                    <a href="{{readmoreurl}}" target="_blank"><strong>Read more</strong></a></p>
                                {{else}}
                                    </p>
                                {{/if}}
                                <p><span class="data-instructions">Created: {{projectcreated}}</span></p>
                            </div>
                        {{else}}
                            <div class="span3">
                                <h4 class="kicker">{{projecttype}}</h4>
                                <img src="https://si0.twimg.com/profile_images/1545969777/kpcc-twitter-logo.png" alt="{{projecttitle}}" />
                            </div>
                            <div class="span9">
                                <h3><a href="{{promotedurl}}" target="_blank">{{projecttitle}}</a></h3>
                                <p><span class="data-instructions">By {{projectbylines}}</span></p>
                                <p>{{truncate projectdescription 250 " ... "}}<br />
                                {{#if readmoreurl}}
                                    <a href="{{readmoreurl}}" target="_blank"><strong>Read more</strong></a></p>
                                {{else}}
                                    </p>
                                {{/if}}
                                <p><span class="data-instructions">Created: {{projectcreated}}</span></p>
                            </div>
                        {{/if}}
                    </div>
                </div>
                <hr></hr>
            {{else}}
            {{/is}}
        {{else}}
        {{/if}}
    {{/objects}}


