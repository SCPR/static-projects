<table class="table">
    <thead>
        <th>Year Sold</th>
        <th>Principal</th>
        <th>Payoff</th>
        <th>Principal to Debt</th>
        <th>Years Until Maturity</th>
    </thead>
    <tbody id="debt-table-content">
        {{#objects}}
        <tr id="{{sale_year}}" class="bar-group">
            <td>{{sale_year}}</td>
            <td>{{addCommas cab_principal}}</td>
            <td>{{addCommas cab_debt}}</td>
            <td>{{debt_to_principal}}</td>
            <td>{{maturity_length}}</td>
        </tr>
        {{/objects}}
    </tbody>
</table>