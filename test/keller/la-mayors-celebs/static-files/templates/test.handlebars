<div class="candidate-container">
    <div class="row-fluid">
        <!-- begin candidate -->
        <div class="span6">
            <div class="span2">
                <div class="image-container">
                <img src="http://a.scpr.org/i/73a0c249835022d4713fc14c6df44306/43770-wide.jpg" alt="Eric Garcetti" />
                </div>
            </div>
            <div class="span10">
                {{#objects}}
                    {{#is candidate 'Eric Garcetti'}}
                        <h3>{{candidate}}</h3>
                            {{#donors}}
                                {{#gte donorContribution 700}}
                                    {{donorName}} - {{donorContribution}}<br />
                                {{else}}
                                {{/gte}}
                            {{/donors}}
                        <br />
                    {{/is}}
                {{/objects}}
            </div>
