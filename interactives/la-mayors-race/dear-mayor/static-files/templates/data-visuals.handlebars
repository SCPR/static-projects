                    <p>{{responses9200af1a79cb}}</p>

                    {{#if responsesdb10ff019c5a}}
                        <p><strong>{{questionsdb10ff019c5avalue}}</strong><br />{{responsesdb10ff019c5a}}</p>
                    {{else}}
                    {{/if}}
                    <p class="data-instructions">Submitted {{timeago srsdate}}</p>
                </div>
            <!-- end cell -->
            {{/if}}

        {{#if isLast}}
            </div>
        {{/if}}

    {{/everyNth}}

    </div>
</div>