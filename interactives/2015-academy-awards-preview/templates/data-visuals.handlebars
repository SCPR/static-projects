{{#objects}}
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div id="{{categorySlug}}" class="elements row">
        <h3>{{category}}</h3>
            {{#each nominees}}
                <div class="nominee-container col-12 col-xs-12 col-sm-6 col-md-4 col-lg-3">
                    <div class="info-block">
                        <img id="{{category}}" src="{{movieimage}}" alt="{{nominee}}" />
                        <h5 class="text-center">{{nominee}}</h5>

                        {{#if movie}}
                            <p class="text-center">{{movie}}</p>
                        {{else}}

                        {{/if}}

                        {{#if awardswon}}
                            <p><strong>Awards won:</strong> <span class="italics">{{awardswon}}</span></p>
                        {{else}}
                            <p>&nbsp;</p>
                        {{/if}}
                        {{#if prediction}}
                            <p><strong>Prediction</strong>: {{prediction}} - <span class="italics"><a href="{{predictionlink}}" target="_blank">{{predictionsource}}</a></span></p>
                        {{else}}
                            <p>&nbsp;</p>
                        {{/if}}
                        <p class="centered">
                            {{#if imdbpage}}
                                <a href="{{imdbpage}}" target="_blank">IMDB</a> |
                            {{else}}
                            {{/if}}
                            {{#if trailer}}
                                <a href="{{trailer}}" target="_blank">Movie Trailer</a> |
                            {{else}}
                            {{/if}}
                            {{#if officialsite}}
                                <a href="{{officialsite}}" target="_blank">Official Site</a>
                            {{else}}
                            {{/if}}
                        </p>
                    </div>
                </div>
            {{/each}}
        </div>
    </div>
{{/objects}}