    <!-- begin row -->
    <div class="row-fluid">
        <h3>{{school_name}}</h3>
        <p>
            {{address}}, {{city}}, CA {{zip_code}}<br />
            {{#if school_phone}}
                {{school_phone}}<br />
            {{/if}}
            {{#if website}}
                <a href="{{website}}" target="_blank"><strong>Website</strong></a>
            {{/if}}
        </p>
        <hr>
    </div>
    <!-- end row -->

    <!-- begin row -->
    <div class="row-fluid">
        <div class="span6">
            {{#if contact_name}}
                <p><span><strong>Contact</strong></span><br/>
                {{contact_name}}<br />

            {{#if contact_phone}}
                {{contact_phone}}<br />
            {{/if}}

            {{#if contact_email}}
                {{contact_email}}
            {{/if}}
                </p>
            {{else}}
                <p>&nbsp;</p>
            {{/if}}
        </div>

        <div class="span6">
            <p><span><strong>Partner Language(s)</strong></span><br/>
            {{partner_language_1}}

            {{#if partner_language_2}}
                 | {{partner_language_2}}</p>
            {{else}}
                </p>
            {{/if}}
        </div>
    </div>
    <!-- end row -->

    <!-- begin row -->
    <div class="row-fluid">
        <div class="span12">
            {{#is hide_from_display 'Yes'}}
                <hr>
            {{else}}
                <p><span><strong>Program Availability</strong></span><br/>
                <table class="table table-bordered table-striped">
                  <thead>
                    <tr>
                        <th>K</th>
                        <th>1</th>
                        <th>2</th>
                        <th>3</th>
                        <th>4</th>
                        <th>5</th>
                        <th>6</th>
                        <th>7</th>
                        <th>8</th>
                        <th>9</th>
                        <th>10</th>
                        <th>11</th>
                        <th>12</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                        {{#is grade_K 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_1 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_2 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_3 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_4 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_5 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_6 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_7 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_8 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_9 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_10 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_11 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}

                        {{#is grade_12 'Yes'}}
                            <td>&#x2713;</td>
                        {{else}}
                            <td>-</td>
                        {{/is}}
                    </tr>
                  </tbody>
                </table>
                <hr>
            {{/is}}
        </div>
    </div>
    <!-- end row -->

    <!-- begin row -->
    <div class="row-fluid">
        <div class="span12">
            <div class="span6">
                <ul>
                    {{#if year_program_started}}
                        <li>Program Started in {{year_program_started}}</li>
                    {{else}}
                    {{/if}}

                    {{#if EL}}
                        <li>Has {{EL}} students classified as English Learners</li>
                    {{else}}
                    {{/if}}

                    {{#if third_language}}
                        <li>Offers {{third_language}} as a 3rd Language.</li>
                    {{else}}
                    {{/if}}
                </ul>
            </div>

            <div class="span6">
                <ul>
                    {{#if since_when}}
                        <li>Earned Biliteracy Seal in {{since_when}}</li>
                    {{else}}
                    {{/if}}

                    {{#if IEP}}
                        <li>Has {{IEP}} students in an Individualized Education Program.</li>
                    {{else}}
                    {{/if}}

                    {{#if students_on_free_reduced_lunch}}
                        <li>{{students_on_free_reduced_lunch}}% receive free/reduced lunch</li>
                    {{else}}
                    {{/if}}
                </ul>
            </div>

        </div>
    </div>
    <!-- end row -->

    <!-- begin row -->
    <div class="row-fluid">
        <div class="span12">
            {{#if program_model}}
                <p><strong>Program Model</strong>: {{program_model}}</p>
            {{else}}
            {{/if}}

            {{#if special_recognition}}
                <p><strong>Recognition/Achievements</strong>: {{special_recognition}}</p>
            {{else}}
            {{/if}}

            <p><a href="#" class="btn btn-success hidden-phone hidden-tablet" onclick="clickModal();">Change/update school information</a></p>

        </div>
    </div>
    <!-- end row -->

